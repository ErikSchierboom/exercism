.globl trinary_convert

trinary_convert:
    move $t0, $a0
    li   $v0, 0
    li   $t2, 48 # ASCII value of '0'
    li   $t3, 3
__loop:
    lb   $t1, ($t0)
    beqz $t1, __done
    sub  $t1, $t1, $t2
    mul  $v0, $v0, $t3
    add  $v0, $v0, $t1
    addi $t0, $t0, 1
    j    __loop
__done:
    jr   $ra