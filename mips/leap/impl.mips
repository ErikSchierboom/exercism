.globl is_leap_year

.text

is_leap_year:
        li $v0, 0
        rem $t0, $a0, 4
        bne $t0, $v0, __is_not_leap_year
        rem $t0, $a0, 400
        beq $t0, $v0, __is_leap_year
        rem $t0, $a0, 100
        beq $t0, $v0, __is_not_leap_year
        b __is_leap_year        
__is_leap_year: 
        li $v0, 1
        jr $ra
__is_not_leap_year: 
        li $v0, 0
        jr $ra
