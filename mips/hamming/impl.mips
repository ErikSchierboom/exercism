.globl hamming_distance

hamming_distance:
    li   $v0, 0
    move $t0, $a0
    move $t1, $a1
__loop:
    lb   $t2, 0($t0)
    lb   $t3, 0($t1)
    beq  $t3, $zero, __done
    beq  $t2, $t3, __next
    addi $v0, $v0, 1
__next:    
    addi $t0, $t0, 1
    addi $t1, $t1, 1
    j    __loop
__done:
    jr   $ra
